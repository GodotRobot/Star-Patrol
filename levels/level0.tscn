script/source = "extends RigidBody2D

onready var up_map_button = get_node(\"Keymap/UpMapButton\")
onready var up_mapping = get_node(\"Keymap/UpMapping\")
onready var up_mapping_text = up_mapping.get_text()

func _ready():
	set_fixed_process(true)
	
func _fixed_process(delta):
	if Input.is_action_pressed(\"ui_right\"):
		apply_impulse(Vector2(0, -10), Vector2(5, 0))
	if Input.is_action_pressed(\"ui_left\"):
		apply_impulse(Vector2(0, -10), Vector2(-5, 0))
	if Input.is_action_pressed(\"ui_up\"):
		apply_impulse(Vector2(0, 0), Vector2(0, -5))

func _on_UpMapButton_pressed():
	up_mapping.set_text(\"press\")
	set_process_input(true)

# will be called only during action capture (remapping)
func _input(event):
	if event.type == InputEvent.MOUSE_MOTION:
		return
	if event.type == InputEvent.MOUSE_BUTTON:
		set_process_input(false)
		up_mapping.set_text(up_mapping_text)
		return
	if event.type == InputEvent.KEY:
		up_mapping_text = OS.get_scancode_string(event.scancode)
	if event.type == InputEvent.JOYSTICK_MOTION:
		var val = '+'
		if event.value < 0:
			val = '-'
		up_mapping_text = 'joy axis ' + val + str(event.axis)
	if event.type == InputEvent.JOYSTICK_BUTTON:
		up_mapping_text = 'joy button ' + str(event.button_index)
	up_mapping.set_text(up_mapping_text)
	InputMap.erase_action('ui_up')
	InputMap.add_action('ui_up')
	InputMap.action_add_event('ui_up', event)
	set_process_input(false)
"

